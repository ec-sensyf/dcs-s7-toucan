<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="application">
  <jobTemplates>
    <jobTemplate id="template">
      <streamingExecutable>/application/node/run.py</streamingExecutable>
            <parameters>
                <parameter id="chlconc">0.045</parameter>
                <parameter id="outputdir">/application/output/RS/chl0045</parameter>
            </parameters>
    </jobTemplate>
  </jobTemplates>
    <workflow id="workflow" title="Basic Workflow" abstract="Basic archetype workflow for a python application">
    <workflowVersion>1.0</workflowVersion>
    <node id="node">
      <job id="template"/>
      <sources>
        <source refid="string:list">input-1,input-2</source>
      </sources>
    </node>
  </workflow>
</application>

